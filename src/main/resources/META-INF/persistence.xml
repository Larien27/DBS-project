<persistence xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.2"
             xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_2.xsd">
    <persistence-unit name="examplePU">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <class>Passenger</class>
        <class>cz.cvut.fel.dbs.HudebniFestival</class>
        <class>cz.cvut.fel.dbs.JeOrganizovan</class>
        <class>cz.cvut.fel.dbs.Jmeno</class>
        <class>cz.cvut.fel.dbs.JsouHrany</class>
        <class>cz.cvut.fel.dbs.JsouHranyId</class>
        <class>cz.cvut.fel.dbs.Listek</class>
        <class>cz.cvut.fel.dbs.Nadrizeny</class>
        <class>cz.cvut.fel.dbs.Navstevnik</class>
        <class>cz.cvut.fel.dbs.Organizator</class>
        <class>cz.cvut.fel.dbs.Pisnicka</class>
        <class>cz.cvut.fel.dbs.PisnickaId</class>
        <class>cz.cvut.fel.dbs.Podium</class>
        <class>cz.cvut.fel.dbs.Predvadi</class>
        <class>cz.cvut.fel.dbs.PredvadiId</class>
        <class>cz.cvut.fel.dbs.Sponzor</class>
        <class>cz.cvut.fel.dbs.Stanek</class>
        <class>cz.cvut.fel.dbs.StanekId</class>
        <class>cz.cvut.fel.dbs.Ucinkujici</class>
        <class>cz.cvut.fel.dbs.Vystoupeni</class>
        <class>cz.cvut.fel.dbs.VystoupeniId</class>

        <properties>
            <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver" />
            <property name="javax.persistence.jdbc.url" value="URL DELETED" />
            <property name="javax.persistence.jdbc.user" value="USERNAME DELETER" />
            <property name="javax.persistence.jdbc.password" value="PASSWORD DELETED" />

            <!-- create database schema from entities-->
            <property name="javax.persistence.schema-generation.database.action" value="create" />
        </properties>
    </persistence-unit>
</persistence>
